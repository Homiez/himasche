= form_for @user do |f|
  -if @user.errors.any?
    #error_explanation
      %h2= "#{pluralize(@user.errors.count, "error")} prohibited this user from being saved:"
      %ul
        - @user.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :tid
    = f.text_field :tid
  .field
    = f.label :img
    = f.text_field :img
  .field
    = f.label :token
    = f.text_field :token
  .field
    = f.label :lastUpdate
    = f.date_select :lastUpdate
  .actions
    = f.submit 'Save'
